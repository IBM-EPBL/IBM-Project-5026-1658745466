apiVersion: apps/v1
	kind: Deployment
	metadata:
	  name: flask-app
	

	spec:
	  replicas: 3
	  selector:
	    matchLabels:
	      app: flask-app
	  template:
	    metadata:
	      labels:
	        app: flask-app
	

	    spec:
	      containers:
	        - name: page
	          image: vaishnavi/flask
	          imagePullPolicy: Never
	          ports:
	            - containerPort: 5000
	              protocol: TC
